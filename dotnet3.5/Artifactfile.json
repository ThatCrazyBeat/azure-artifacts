{
    "$schema": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/schemas/2015-01-01/dtlArtifacts.json",
    "title": ".NET Framework 3.5",
    "description": "Enable Microsoft .NET Framework 3.5 using PowerShell Cmdlet",
    "tags": [
        "Windows",
        ".Net Framework"
    ],
    "iconUri": "https://raw.githubusercontent.com/Azure/azure-devtestlab/master/Artifacts/windows-dotnet45/dotnet.png",
    "targetOsType": "Windows",
    "parameters": {
        "installUsername": {
            "type": "string",
            "displayName": "Install User Name",
            "description": "User with admin rights to install OfficeProPlus2013"
        },
        "installPassword": {
            "type": "securestring",
            "displayName": "Install Password",
            "description": "Password for the install user"
        }
    },
    "runCommand": {
        "commandToExecute": "powershell.exe -ExecutionPolicy bypass -File startChocolatey.ps1"
    }
